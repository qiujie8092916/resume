$nav : navbar;
$white : #fff;
$black : #333;

@mixin animate($attrName, $customName, $duration, $timingFunction: ease){
	-webkit-#{$attrName}: $customName $duration;
	-moz-#{$attrName}: $customName $duration;
	-ms-#{$attrName}: $customName $duration;
	-o-#{$attrName}: $customName $duration;
	#{$attrName}: $customName $duration;
}

@mixin transform($keyname){
	-webkit-transform: $keyname;
	-moz-transform: $keyname;
	-ms-transform: $keyname;
	-o-transform: $keyname;
	transform: $keyname;
}

@mixin box-shadow($h, $v, $spread, $color){
	-webkit-box-shadow: $h $v $spread $color;
	-moz-box-shadow: $h $v $spread $color;
	-ms-box-shadow: $h $v $spread $color;
	-o-box-shadow: $h $v $spread $color;
	box-shadow: $h $v $spread $color;
}

@mixin border-radius($value){
	-webkit-border-radius: $value;
	-moz-border-radius: $value;
	-ms-border-radius: $value;
	-o-border-radius: $value;
	border-radius: $value;
}

*{
	margin: 0;
	padding: 0;
}
/*
html{-webkit-text-size-adjust:none;}
*/
body{
	font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;;
}

dt{
	font-weight: inherit;
}

h1, h2, h3, h4, h5, h6{
	display: inline-block;
}

h4.cover-heading{
	margin:{
		top: 10px;
		bottom: 10px;	
	}
}

h5{ 
	font-size: 15px; 
	text-align: left
}

hr{
	margin:{
		top: 8px;
		bottom: 8px;	
	}
}

.cover{padding: 7% 20px 0 20px;}
footer{
	background: #555;
	padding: 70px 0;    
	bottom: 0;
	position: absolute;
	width: 100%;
	
	p{
		margin:0 !important;    
		height: 25px;
    width: 100%;
	}
	#github{
		img{
			vertical-align: bottom;
		}
		&::hover,
		&:focus{
			text-decoration: none;
		}
	}
}

img{ 
	border: 0; 
	max-width: 100%;
}

nav{
	display: -webkit-box;
	background: rgba(245, 245, 245, 0.8);
	color: $black !important;
	@box-shadow(0, 0, 10px, rgba(51 ,51, 51, 0.6));
	
	ul.navbar-nav li a i{
		display: none;
	}
}

@for $i from 1 to 6{
	@if $i == 1{
		ul.#{$nav}-right.nav.#{$nav}-nav > li:nth-child(#{$i}), #navSelected{
			@include animate(animation, navMotion, calc(1.3s + #{$i} * 0.2s))
		}
	} @else{
		ul.#{$nav}-right.nav.#{$nav}-nav > li:nth-child(#{$i}){
				@include animate(animation, navMotion, calc(1.3s + #{$i} * 0.2s))
		}
	}
}

.btn_custom{
	color: $white; 
	background-color: rgba(255, 255, 255, 0.3); 
	font-weight: bold; 
	border: 1px solid transparent; 
	padding: 8px 60px; 
	@include border-radius(6px); 
	position: absolute; 
	top: -38px;
	left: 50%;
	margin-left: -87.275px;
	outline: none;
	@include animate(transition, all, .4s);
	&:hover{
		background-color: transparent; 
  	border: 1px solid #fff;
	}
}

.container-timeline{
	width: 65%;
	margin: 0 auto;	
}

.cd-timeline-block {
	.cd-timeline-img.work-backgroundColor{
		background-color: #75ce66
	}
	.cd-timeline-img.study-backgroundColor{ 
		background-color: #c03b44
	}
	.cd-timeline-content ul li{ 
		list-style: none; 
		margin: 3px 0;
	}
}

.cover-heading{
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.4);
	@include animate(animation, headerMotion, 1s)
}

.decoration{
	width: 0; 
	height: 0; 
	margin: 0 auto 3px auto;
	@include animate(transition, width, 1s);
	&:after{ 
		content:""; 
		display: block; 
		border-bottom:1px solid $white;
	}
}

.demo1Btn{ 
	width: 50px;
	height: 50px; 
	line-height: 50px; 
	border: 2px solid rgba(255, 255, 255, 0.5); 
	text-align: center; 
	right: 50px; 
	bottom: 50px; 
	z-index: 99; 
	position: fixed; 
	font-weight: 400; 
	display: none; 
	color: $white; 
	cursor: pointer; 
	overflow: hidden;
	@include border-radius(50px);
	@include animate(transition, all, 0.5s);
	&:hover{
		border: 2px solid rgba(255, 255, 255, 1);
		.first-link{
			margin-top: -50px
		}
	}
	.first-link{ 
		margin-top: 0;   
		@include animate(transition, all, 0.5s);
	}
	a{
		color: white; 
		text: {
			decoration: none;
			align: center; 
		}
		display: block;
		&:hover{
			text-decoration:none
		}
	}
}

.divider{ 
	border-bottom: 1px solid transparent; 
	height: 1px;
	display: block; 
	margin: 3px 0;
}

.fsize{ 
	font-size: 17px;
}

.fix-width{
	width: 1200px;
	margin: 0 auto;	
}

.homebackground{ 
	overflow: hidden; 
	/*padding-top: 7%; */
	margin-top: 0; 
	width: 100%; 
	position: relative;
	&:first-child{
		.inner.cover{
			margin-top: 13%;
		}
	}
	@include animate(transition, all, 0.25s, ease-in-out);
	h3{
		text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.4);	
	}
	a:focus, 
	a:hover{
		color: inherit !important;
	}
	.article{
		width: 35%;
		min-width: 400px;
	}
}

.icon_div{ 
	position: relative; 
	margin-top: 10%;
	.ski_icon{
		position: relative;
		.li_icon{
			position: absolute;
			text-align: center;
			visibility: hidden; 
			padding: 0;
			@include animate(transition, all, .5s);
			&:nth-child(1){
				left: 0
			}
			&:nth-child(2){
				left: calc((100% - 240px) / 3 + 60px)
			}
			&:nth-child(3){
				left: calc(2 * (100% - 240px) / 3 + 120px)
			}
			&:nth-child(4){
				right: 0
			}
			.icon{ 
				font-size: 60px; 
			/*line-height: 100px;
				clear: both;*/
				&:hover{
					cursor: pointer;	
				}
			}
		}
	}
	& .icon_detail{
	/*height: 100%;*/
		width: calc(100% - 15px); 
		position: absolute;
		top: 100%; 
		left: 1%;
		background-color: rgba(255, 255, 255, 1);
		@include border-radius(4px);
		color: $black; 
		padding: 3px 0; 
		box-shadow: 0 0 10px rgba(51, 51, 51, 0.6); 
		visibility: hidden;
		@include animate(transition, all, 1s);
		.detailImg{
			margin-bottom: 0;
			li{
				margin: 5px 0; 
				font-size: 13px; 
				text-shadow: 1px 1px 0px rgba(0,0,0,0.1);
			}
		}
	}
}


.intro{
	position: relative;
	padding: 0.5% 0.5%;
	margin-top: calc(100px + 10%);
	width: 80%;
	overflow: hidden;
	&:before{ 
		top: 0; 
		left: 0;
		border: {
			top: 2px solid $white;
			left: 2px solid $white;
		}
	}
	&:after{
		bottom: 0; 
		right: 0;
		border: {
			right: 2px solid $white;
			bottom: 2px solid $white;
		}
	}
	&:before, 
	&:after{
		position:absolute; 
		width: 7%; 
		height: 20%; 
		content: "";
	}
	.fsize_{ 
		font-size:15px; 
		position: relative; 
		bottom: -300px; 
		line-height: 30px
	}
}

.lead{ 
	margin-top: 10px;
}

.legend{
	.r_legend{ 
		padding-left: 7px !important;
	}
	&:not(.r_legend){ 
		padding: {
			left: 7px !important;
			right: 10px !important;
		}
	}
}

.logo{ 
	display: inline-block; 
	margin-right:20px; 
	float: left; 
	height: 60px; 
	width: 60px; 
	position: relative;
	img{
		margin-top: 5px; 
		@include border-radius(30px); 
		position: absolute; 
		@include animate(transition, box-shadow, .5s);
		left: 0;
		&:hover{ 
			box-shadow: 0 0 10px #1FAEFF;
		}
	}
	#persion2{ 
		opacity: 0 
	}
}

.major{
	position:absolute; 
	top: 28%;
	width: 1200px;
  margin-left: calc(50% - 600px);
	
	p{
		display: table; 
		height: 190px; 
		position: relative; 
		text-align: left; 
		font-size: 13.5px;
	}
	img{
		/*opacity: 0.8;*/
		@include animate(transition, all, 0.3s);
	}
	img:hover{
		/*opacity: 1 !important;*/
		@include transform(translate3d(0, -3px, 0));
		@include box-shadow(0, 15px, 30px, rgba(0, 0, 0, 0.1));
	}
	.background-shadow{
		height: 190px;
		width: 384px;
		position: absolute;
		top: 0;
		left: 0;
		overflow: hidden;
		@include animate(transition, all, 0.2s);
	}
	.pro-module{
		display: inline-block;
		@include animate(transition, all, 0.3s);
		/*cursor: pointer;*/
		&:hover{
			img{
				@include transform(translate3d(0, -3px, 0));
				@include box-shadow(0, 15px, 30px, rgba(0, 0, 0, 0.1));
			}
			.background-shadow{
				@include transform(translate3d(0, -3px, 0));
				@include box-shadow(0, 15px, 30px, rgba(0, 0, 0, 0.1));
				background-color: rgba(0, 0, 0, .3);
				.btn_custom{
					top: calc(50% - 19px);	
				}
			}
		}
	}
}
 
.#{$nav}-default .#{$nav}-nav{
	& > li > a { 
		color: $black;
	}
	& > .active > a{
		color: $black; 
		background-color: transparent !important;
	}
}

.#{$nav}-brand{
	display: block;
	color: $black !important;
	&:hover{
		@include animate(animation, bounce, .7s)	;
	}
}

