#cd-timeline{
	&::before{ 
		width: 0px; 
		@include animate(transition, width, .6s);
	}
	&.before::before{ 
		width: 4px;
	}
}

#fadeWhite{
	position: fixed; 
	width: 100%; 
	background: $white; 
	height: 100%; 
	min-height: 100%; 
	left: 0px; 
	top: 0px; 
	z-index: 99999;
}

#navSelected{ 
	position: absolute; 
	top: 75px; 
	height: 5px; 
	line-height: 33px;
	&:after{ 
		content: ""; 
		display: block; 
		border-bottom: 2px solid $white
	}
}

#triangle_up{ 
	width: 0; 
	height: 0; 
	border: {
		left: 10px solid transparent;
		right: 10px solid transparent;
		bottom: 20px solid rgba(255, 255, 255, 1);
	}
	position: absolute; 
	top: -20px; 
	left: 10px;
	@include animate(transition, all, .5s);
}


/**************************
			*PHONE*
*************************/
@media (max-width: 768px){
	i.iconfont{
		color: $black;
		font-size: 30px;
	}
	
	h4.cover-heading, p.decoration{
		display: none;
	}

	nav.navbar{
		bottom: 0;    
		top: initial;
		background: rgba(245, 245, 245, 1);
		box-shadow: initial;
		
		.navbar-brand.oneng{
			display: none;	
		}
		
		.container{
			#navbar.inner{
				padding: 0;
			}
			
			ul.nav{
			width: 100%;
			display: -moz-box; 
	    display: -webkit-box; 
	    display: box;
			font-weight: initial;
			margin: 0;
			
			& li{
				/*display: inline-block;*/
				-moz-box-flex: 1; 
		    -webkit-box-flex: 1; 
		    box-flex: 1;
				
				&.active a, &.active i{
					color: #00D200;	
				}
				
				a{
					padding-top: 8px;
    			padding-bottom: 0px;
					font-size: 12px;
					color: #B5B4BA;
					line-height: 20px;
					
					span.nav_assist{
						font-size: 10px;
						-webkit-transform-origin-x: 0;
            -webkit-transform: scale(0.5);
						transform: scale(0.5);
					}
				}
				
				i{
					color: #B5B4BA;
					display: block;
				} 
			}
		}
		}
	}
	
	.intro {
		&:before, &:after{
			width: 8%;
	    height: 10%;
		}
		
		&:before{
	    border-top-width: 1px;
	    border-left-width: 1px;	
		}
		
		&:after{
	  	border-right-width: 1px;
	    border-bottom-width: 1px;
		}
		
		.fsize_{
			font-size: 12px;	
		}
	}
	
	.contenttitle{ 
		margin-top: 120px; 
	}
	
	.active{ 
		border-bottom: 0;
	}
	
	.demo1Btn{ 
		right: 5px;
		.first-link{ 
			margin-top: -50px;
		}
	}
	
	.major{
		top: 25%;    
		width: inherit;
    margin-left: 0;
		
		.background-shadow{
			background-color: transparent !important;
			box-shadow: none !important;
			transform: initial !important;
			
			.btn_custom{
				color: $white; 
				background-color: transparent; 
				font-weight: bold; 
				border: 0; 
				position: absolute; 
				top: 0 !important;
				left: 0;
				margin-left: 0;
				padding: 0;
			}
		}
		
		img{
			transform: initial !important;
			box-shadow: none !important;
			height: calc(150px / (340/190));
    	width: 150px;	
		}	
	}
	
	.container-timeline{
		width: 80%;
		margin: 0 auto;	
	}
	
	.top{
		margin-bottom: 12%;	
	}
	
	.left{
		margin-right: 8%;		
	}
	
	.homebackground{
		background-size: contain;
		/*padding-top: 2%;*/
		
		&:first-child{
			p{
				margin-bottom: 20px;	
			}
			.inner.cover{
				margin-top: 40%;
			}	
		}
	
		.article{
			width: 85%;
    	min-width: 0;
		}
		
		.intro {
			margin-top: 110px;	
		}
		
		.icon_div .icon_detail{
			top: 90%;
				
			.detailImg li{
				font-size: 11px;	
			}
		}
	}
	
	.#{$nav}-brand.oneng{
		font-size: 18px;
	}
	
	.resposive_nav{ 
		margin: 0; 
		padding: 0; 
		width: 100%;
		height: 45px;
	}
	
	.webtip{
		display: none;
	}
		
	#{$nav}{ 
		padding: 0; 
		overflow-x: hidden;
	}
	
	#navSelected, .navbar-header{
		display: none
	}
	
	button.navbar-toggle{
		@include animate(transition, background, .9s);
	}
		
	footer{ 
		padding: 25px 0; 
		font-size: 12px;
    bottom: 45px;/*
    position: initial;
    margin-top: 20px;*/
	}
	
	.cd-timeline-content{
		hr{
	    margin-top: 1px;
	    margin-bottom: 5px;
  	}	
	}
	
	.text-left{
    font-size: 11px;
    
		h5{
			font-size: 12px;	
    	line-height: 14px;
		}	
	}
	.cd-date{
		font-size: 11px;
		padding: 0 !important;
	}

	@for $i from 1 to 6{
		ul.#{$nav}-right.nav.#{$nav}-nav > li:nth-child(#{$i}){
			@include animate(animation, navMotion, calc(#{$i} * 0.3s))
		}
	}

}